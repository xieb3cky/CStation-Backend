{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "name": {
            "type": "string",
            "minLength": 3
        },
        "address": {
            "type": "string",
            "minLength": 3
        },
        "lat": {
            "type": "number"
        },
        "long": {
            "type": "number"
        },
        "connection_type": {
            "type": "string"
        },
        "level": {
            "type": "integer"
        },
        "supercharger": {
            "type": "boolean"
        },
        "usage_cost": {
            "type": "string"
        },
        "phone": {
            "type": "string"
        },
        "email": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60,
            "format": "email"
        },
        "available": {
            "type": "integer"
        }
    },
    "additionalProperties": false,
    "required": [
        "id",
        "lat",
        "long"
    ]
}
{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 2,
            "maxLength": 30
        },
        "review": {
            "type": "string",
            "minLength": 2,
            "maxLength": 100
        },
        "rating": {
            "type": "integer"
        }
    },
    "additionalProperties": false,
    "required": [
        "title",
        "review"
    ]
}